#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules/@angular/cli/bin/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules/@angular/cli/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules/@angular/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules/@angular/cli/bin/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules/@angular/cli/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules/@angular/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/@angular+cli@17.2.0/node_modules:/Users/selimgecin/Desktop/connect-ion-projects/firstProject/front/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@angular/cli/bin/ng.js" "$@"
else
  exec node  "$basedir/../@angular/cli/bin/ng.js" "$@"
fi
